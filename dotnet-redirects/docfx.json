{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/azure-dev-docs.ja-jp",
      "feedback_product_url": "https://github.com/Azure/azure-sdk-for-java/issues",
      "ms.topic": "conceptual",
      "ms.devlang": "csharp",
      "ms.date": "09/20/2018",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "Azure.azuredotnet",
          "folder_relative_path_in_docset": "docs-ref-conceptual"
        }
      },
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ]
    },
    "fileMetadata": {},
    "template": [],
    "dest": "azure-dotnet",
    "markdownEngineName": "markdig"
  }
}
